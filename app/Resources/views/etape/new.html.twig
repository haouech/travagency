{% extends 'agvoybase.html.twig' %}


{% block main %}
    <h1>Ajouter Nouvelle Etape</h1>
	
    {{ form_start(form) }}
    {{ form_errors(form) }} 
    {{ form_row(form.numeroEtape, { 'attr': {'class': 'form-control'} }) }}
    {{ form_row(form.villeEtape, { 'attr': {'class': 'form-control'} }) }} 
    {{ form_row(form.nombreJours, { 'attr': {'class': 'form-control'} }) }} 
    {{ form_end(form) }}

{% endblock %}
